          <h1>Режим и питание</h1>
        </div>
    </div>
    <div dir="row">

      <div class="col-md-3 prNone"> <!-- required for floating -->
        <!-- Nav tabs -->
        <ul class="nav nav-tabs tabs-left">
          <li class="active"><a href="#sec1" data-toggle="tab">Питание</a></li>
